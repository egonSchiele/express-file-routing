{
  "name": "express-file-routing",
  "version": "3.1.0",
  "description": "Simple file-based routing for Express",
  "author": "Matthias Halfmann",
  "repository": "egonSchiele/express-file-routing",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && pnpm run templates && tsc",
    "dev": "ts-node-dev --transpile-only --files --quiet ./examples/with-typescript/app.ts",
    "templates": "typestache /Users/adit/express-file-routing/src/templates -v"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "keywords": [
    "express",
    "api",
    "file",
    "router",
    "routing",
    "typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "express": ">4.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.4",
    "eslint": "^8.39.0",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "typestache": "^0.4.4"
  }
}
